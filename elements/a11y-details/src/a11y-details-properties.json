{
  /**
   * optional text for when summary button is open,
   * eg. "Hide", "Less" or "Close"
   */
  "closeText": {
    "type": "String",
    "attribute": "close-text",
    "reflect": true
  },
  /**
   * optional text for when summary button is closed,
   * eg. "Show", "More" or "Open"
   */
  "openText": {
    "type": "String",
    "attribute": "open-text",
    "reflect": true
  }
}
