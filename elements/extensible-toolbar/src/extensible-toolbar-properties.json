{
  /**
   * Is the toolbar collapsed?
   */
  "collapsed": {
    "attribute": "collapsed",
    "reflect": true,
    "type": "Boolean"
  },

  /**
   * Size of the toolbar, as `xs`, `sm`, `md`, `lg`, or `xl`.
   */
  "responsiveSize": {
    "attribute": "responsive-size",
    "type": "String"
  },

  /**
   * Should the toolbar stick to top so that it's always visible?
   */
  "sticky": {
    "attribute": "sticky",
    "type": "Boolean",
    "reflectToAttribute": true
  }
}
