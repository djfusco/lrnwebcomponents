{
  /**
   * ability to edit the items in the list
   */
  "editMode": {
    "type": Boolean,
    "reflect": true,
    "attribute": "edit-mode"
  },
  /**
   * items array
   */
  "items": {
    "type": Array,
    "reflect": false
  },
  /**
   * Active element being worked on in the list
   */
  "activeElement": {
    "type": Object
  }
}
