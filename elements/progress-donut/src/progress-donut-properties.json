{
  /**
   * Timing for animation or -1 for false
   */
  "animation": {
    "type": "Number",
    "attribute": "animation"
  },
  /**
   * Timing for animation or 0 none
   */
  "animationDelay": {
    "type": "Number",
    "attribute": "animation-delay"
  },
  /**
   * An array of completed values
   */
  "complete": {
    "type": "Array"
  },
  /**
   * Accessible long description
   */
  "desc": {
    "type": "String"
  },
  /**
   * Source of image in the center of the object.
   */
  "imageSrc": {
    "attribute": "image-src",
    "type": "String",
    "reflect": true
  },
  /**
   * Alt text for image.
   */
  "imageAlt": {
    "attribute": "image-alt",
    "type": "String",
    "reflect": true
  }
}
